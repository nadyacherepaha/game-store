{
  "name": "webpack-must-have",
  "version": "0.0.2",
  "description": "Must-have config for Webpack 4",
  "main": "main.jsx",
  "scripts": {
    "test": "npm run lint",
    "serve": "./node_modules/.bin/webpack-dev-server --hot --open --config webpack.devServer.js",
    "build-prod": "./node_modules/.bin/webpack --mode production --config webpack.prod.js",
    "build-prod-withMap": "./node_modules/.bin/webpack --mode production --config webpack.prod.js --sourceMap",
    "build-dev": "./node_modules/.bin/webpack --mode development --config webpack.dev.js",
    "lint": "npm run lint-code && npm run lint-styles",
    "lint-code": "./node_modules/.bin/eslint --fix ./**/*.{js,jsx,ts,tsx,json}",
    "lint-styles": "./node_modules/.bin/stylelint --fix ./**/*.{css,scss,less}"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Yegorich555/WebpackMustHave.git"
  },
  "keywords": [
    "React",
    "Webpack",
    "ReactJS",
    "Webpack4"
  ],
  "author": "Yegor.Golubchik",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Yegorich555/WebpackMustHave/issues"
  },
  "homepage": "https://github.com/Yegorich555/WebpackMustHave#readme",
  "devDependencies": {
    "@babel/core": "^7.7.2",
    "@babel/plugin-proposal-class-properties": "^7.7.0",
    "@babel/preset-env": "^7.7.1",
    "@babel/preset-react": "^7.7.0",
    "@babel/preset-typescript": "^7.7.4",
    "autoprefixer": "^9.7.1",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-jsx-classnames-advanced": "^1.0.2",
    "case-sensitive-paths-webpack-plugin": "^2.2.0",
    "clean-webpack-plugin": "^3.0.0",
    "compression-webpack-plugin": "^3.0.1",
    "copy-webpack-plugin": "^5.0.5",
    "css-loader": "^3.2.0",
    "eslint": "^6.6.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.5.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-json": "^2.0.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "file-loader": "^4.2.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-class-name": "^0.6.0",
    "mini-css-extract-plugin": "^0.7.0",
    "node-sass": "^4.13.0",
    "obsolete-webpack-plugin": "^0.5.6",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "postcss-normalize": "^8.0.1",
    "postcss-scss": "^2.0.0",
    "preload-webpack-plugin": "^3.0.0-beta.3",
    "prettier": "^1.19.1",
    "prettier-eslint": "^9.0.0",
    "sass-loader": "^8.0.0",
    "script-ext-html-webpack-plugin": "^2.1.4",
    "style-loader": "^0.23.1",
    "stylelint": "^11.1.1",
    "stylelint-config-prettier": "^6.0.0",
    "stylelint-prettier": "^1.1.1",
    "svg-url-loader": "^3.0.2",
    "terser-webpack-plugin": "^2.3.0",
    "url-loader": "^2.2.0",
    "webpack": "^4.41.3",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  }
