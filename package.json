    "build-dev": "./node_modules/.bin/webpack --mode development --config webpack.dev.js",
    "build": "npm run build-prod",
    "lint": "./node_modules/.bin/eslint --fix ./**/*.js ./**/*.jsx ./**/*.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Yegorich555/FromScratch_React.git"
